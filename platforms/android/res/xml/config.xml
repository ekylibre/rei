<?xml version='1.0' encoding='utf-8'?>
<widget id="org.ekylibre.rei" version="0.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <name>Rei</name>
    <description>
    Mobile app for tracking intelligency
  </description>
    <author email="contact@ekylibre.org" href="http://www.ekylibre.org">
    Ekylibre Team
  </author>
    <feature name="http://api.phonegap.com/1.0/device" />
    <feature name="http://api.phonegap.com/1.0/geolocation" />
    <feature name="http://api.phonegap.com/1.0/notification" />
    <gap:plugin name="com.phonegap.plugins.barcodescanner" version="1.1.0" />
    <preference name="permissions" value="none" />
    <preference name="phonegap-version" value="3.4.0" />
    <preference name="orientation" value="portrait" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="true" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="10000" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="auto-hide-splash-screen" value="true" />
    <preference name="disable-cursor" value="false" />
    <preference name="android-minSdkVersion" value="7" />
    <preference name="android-installLocation" value="auto" />
    <icon src="../config/resources/icon/default.png" />
    <icon gap:density="ldpi" gap:platform="android" src="../config/resources/icon/android/ldpi.png" />
    <icon gap:density="mdpi" gap:platform="android" src="../config/resources/icon/android/mdpi.png" />
    <icon gap:density="hdpi" gap:platform="android" src="../config/resources/icon/android/hdpi.png" />
    <icon gap:density="xhdpi" gap:platform="android" src="../config/resources/icon/android/xhdpi.png" />
    <icon gap:platform="blackberry" src="../config/resources/icon/blackberry/default.png" />
    <icon gap:platform="blackberry" gap:state="hover" src="../config/resources/icon/blackberry/default.png" />
    <icon gap:platform="ios" height="57" src="../config/resources/icon/ios/iphone.png" width="57" />
    <icon gap:platform="ios" height="72" src="../config/resources/icon/ios/ipad.png" width="72" />
    <icon gap:platform="ios" height="114" src="../config/resources/icon/ios/iphone-retina.png" width="114" />
    <icon gap:platform="ios" height="144" src="../config/resources/icon/ios/ipad-retina.png" width="144" />
    <icon gap:platform="tizen" src="../config/resources/icon/tizen/default.png" />
    <icon gap:platform="webos" src="../config/resources/icon/webos/default.png" />
    <icon gap:platform="winphone" src="../config/resources/icon/windows-phone/default.png" />
    <icon gap:platform="winphone" gap:role="background" src="../config/resources/icon/windows-phone/tile.png" />
    <gap:splash src="../config/resources/screen/default.png" />
    <gap:splash gap:density="ldpi" gap:platform="android" src="../config/resources/screen/android/ldpi-portrait.png" />
    <gap:splash gap:density="mdpi" gap:platform="android" src="../config/resources/screen/android/mdpi-portrait.png" />
    <gap:splash gap:density="hdpi" gap:platform="android" src="../config/resources/screen/android/hdpi-portrait.png" />
    <gap:splash gap:density="xhdpi" gap:platform="android" src="../config/resources/screen/android/xhdpi-portrait.png" />
    <gap:splash gap:platform="blackberry" src="../config/resources/screen/blackberry/default.png" />
    <gap:splash gap:platform="ios" height="480" src="../config/resources/screen/ios/iphone-portrait.png" width="320" />
    <gap:splash gap:platform="ios" height="960" src="../config/resources/screen/ios/iphone-retina-portrait.png" width="640" />
    <gap:splash gap:platform="ios" height="1024" src="../config/resources/screen/ios/ipad-portrait.png" width="768" />
    <gap:splash gap:platform="ios" height="768" src="../config/resources/screen/ios/ipad-landscape.png" width="1024" />
    <gap:splash gap:platform="winphone" src="../config/resources/screen/windows-phone/portrait.jpg" />
    <access origin="*" />
</widget>
